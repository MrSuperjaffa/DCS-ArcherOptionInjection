	<Weather>
		<!-- SETTING VALUES HERE WILL OVERWRITE THE REAL WEATHER --> 
		<Temperature_C></Temperature_C> 
		<Altimeter_InHG></Altimeter_InHG>
		<!-- Winds (Upper winds are calculated from ground winds.) -->
		<WindGroundSpeedKts></WindGroundSpeedKts>
		<WindGroundDir></WindGroundDir>
		<Wind2000SpeedKts></Wind2000SpeedKts>
		<Wind2000Dir></Wind2000Dir>
		<Wind8000SpeedKts></Wind8000SpeedKts>
		<Wind8000Dir></Wind8000Dir>
		<Turbulence></Turbulence> <!-- -->
		<!-- Clouds and Precipitation -->
		<CloudBase_FtMSL></CloudBase_FtMSL> <!-- -->
		<CloudHeight_Ft></CloudHeight_Ft> <!-- -->
		<CloudCoverage></CloudCoverage> <!-- 0 - 10 -->
		<Precipitation></Precipitation> <!-- 0=None;1=Rain;2=Thunderstorm;3=Snow;4=Snowstorm -->
		<!-- Fog -->
		<FogThickness_Ft></FogThickness_Ft> <!-- -->
		<FogVisibility_NM></FogVisibility_NM> <!-- 0 - 3 -->
		<!-- Dust -->
		<DustVisibility_Ft></DustVisibility_Ft>
		<WeatherPreset></WeatherPreset>
	</Weather>
	<Time>
		<!-- SETTING VALUES HERE WILL OVERWRITE THE REAL TIME -->
		<Year></Year>
		<Month></Month>
		<Day></Day>
		<Time></Time> <!-- HH:MM:SS --> 
		<Timezone></Timezone>
	</Time>
	<Constraints>
		<!-- Real Weather Constraints. User weather bypasses these. -->
